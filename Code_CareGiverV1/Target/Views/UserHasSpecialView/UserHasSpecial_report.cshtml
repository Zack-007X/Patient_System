@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@{
    ViewData["Title"] = "UserHasSpecial";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>@Configuration["SiteInformation:modulename"]</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item "><a href="@Configuration["SiteInformation:mainsite"]">หน้าแรก</a></li>
                    <li class="breadcrumb-item "><a href="@Configuration["SiteInformation:mainsite"]@Configuration["SiteInformation:appsite"]">@Configuration["SiteInformation:modulename"]</a></li>
                    <li class="breadcrumb-item active">UserHasSpecial</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
    <div class="container-fluid">

        <!-- general form elements -->
        <div class="card card-primary">
            <div class="card-body">
                <div class="row">


                        <div class="form-group col-md-3">
                            <label id='lab_s_UserHasSpecial_masterRoleId' for='s_UserHasSpecial_masterRoleId'>รหัสกลุ่มผู้ใช้</label>
                            <select class="form-control" id="s_UserHasSpecial_masterRoleId" iLabel="รหัสกลุ่มผู้ใช้" iRequire="true" iGroup="s_UserHasSpecial" title='รหัสกลุ่มผู้ใช้' placeholder='รหัสกลุ่มผู้ใช้'></select>
                        </div>

                        <div class="form-group col-md-3">
                            <label id='lab_s_UserHasSpecial_specialSkill' for='s_UserHasSpecial_specialSkill'>ความเชี่ยวชาญ</label>
                            <input class="form-control" type="text" id="s_UserHasSpecial_specialSkill" iLabel="ความเชี่ยวชาญ" iRequire="true" iGroup="s_UserHasSpecial" title='ความเชี่ยวชาญ' placeholder='ความเชี่ยวชาญ' />
                        </div>
  


                    <div class="form-group col-sm-6">
                        <button class="btn btn-primary" onclick="javascript:UserHasSpecial_DoSearch('pdf');">แสดงรายงาน</button>
                        <button class="btn btn-primary" onclick="javascript:UserHasSpecial_DoSearch('xlsx');">ดาวน์โหลดเป็น Excel</button>
                    </div>
                </div>
            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.card -->
        <!-- general form elements -->
        <div class="card card-primary">
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <iframe id="report_result" style="display:none; height:500px; width:100%;"></iframe>
                    </div>
                </div>
            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.card -->

    </div><!-- /.container-fluid -->

</section>
<!-- /.content -->

@section FooterPlaceHolder{
    <script src="~/js/UserHasSpecial/UserHasSpecial_report.js"></script>
    <script>
        $(document).ready(function () {
            UserHasSpecial_InitialForm();
            SetupValidationRemark("s_UserHasSpecial");
        });
    </script>
}

